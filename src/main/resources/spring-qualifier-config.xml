<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>

    <bean id="profileForQualifier" class="com.liugs.tool.spring.annotations.ProfileForQualifier" />

    <bean id="demoForQualifier" class="com.liugs.tool.spring.annotations.DemoForQualifier">
        <property name="name" value="BOMA"/>
        <property name="age" value="24"/>
        <property name="sex" value="man"/>
    </bean>

    <bean id="demoForQualifier1" class="com.liugs.tool.spring.annotations.DemoForQualifier">
        <property name="name" value="Rolls-Royce"/>
        <property name="age" value="24"/>
        <property name="sex" value="car"/>
    </bean>

</beans>